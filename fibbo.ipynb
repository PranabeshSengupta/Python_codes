    "def fibbo(n):\n",
    "    prev1=1\n",
    "    prev2=0\n",
    "    if n==0 or n==1:\n",
    "        return n\n",
    "    for i in range(2,n+1):\n",
    "        curr=prev1+prev2\n",
    "        prev2=prev1\n",
    "        prev1=curr\n",
    "    return prev1\n",
    "\n",
    "#calling......\n",
    "n=int(input(\"Enter your no\"))\n",
    "fibbo(n)"
   ]
